{
  "name": "spend-campaign-be",
  "private": true,
  "version": "1.0.0",
  "description": "Spend Campaign Backend",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/src/cli.js",
    "run-script": "ts-node -r tsconfig-paths/register src/scripts/$npm_config_name",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate -p -d ormconfig.ts migrations/$npm_config_name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts seed",
    "send-mail": "ts-node -r tsconfig-paths/register src/cmd.ts job:send-mail",
    "generate:operator": "ts-node -r tsconfig-paths/register src/shared/scripts/generate-operator.ts",
    "consumer": "ts-node -r tsconfig-paths/register src/consumer.ts",
    "crawler": "ts-node -r tsconfig-paths/register src/crawler.ts",
    "local": "ts-node -r tsconfig-paths/register src/local.ts",
    "scheduler": "ts-node -r tsconfig-paths/register src/scheduler.ts"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.687.0",
    "@fireblocks/ts-sdk": "^4.0.0",
    "@jm18457/kafkajs-msk-iam-authentication-mechanism": "^3.1.2",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.2.5",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.5",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.4.3",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^11.0.13",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.1.11",
    "@nestjs/typeorm": "^10.0.0",
    "@types/uuid": "^9.0.4",
    "bcrypt": "^5.1.1",
    "bignumber.js": "^4.0.4",
    "bull": "^4.16.3",
    "cache-manager": "^5.6.1",
    "cache-manager-ioredis-yet": "^2.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "ethers": "^6.13.3",
    "fast-csv": "^5.0.1",
    "firebase-admin": "^12.6.0",
    "install": "^0.13.0",
    "ioredis": "^5.4.1",
    "joi": "^17.10.1",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "nest-commander": "^3.15.0",
    "node-telegram-bot-api": "^0.66.0",
    "nodejs-snowflake": "^2.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "5.0.0",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.6.0",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "npm": ">=10.8.1",
    "node": ">=20.16.0"
  },
  "overrides": {
    "tough-cookie": "^4.1.3"
  }
}
